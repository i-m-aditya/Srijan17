<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.srijan.Contact_us">

    <android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/activity_contact_us"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.testimonial.ContactUs"
        android:background="@drawable/back2">
        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/stat_sys_phone_call"
            android:id="@+id/imageView2"
            tools:layout_conversion_absoluteX="225dp"
            tools:layout_conversion_absoluteY="216dp"
            tools:layout_conversion_absoluteWidth="24dp"
            tools:layout_conversion_absoluteHeight="24dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            app:layout_constraintVertical_bias="0.42000002" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_dialog_email"
            android:id="@+id/imageView"
            tools:layout_conversion_absoluteX="99dp"
            tools:layout_conversion_absoluteY="208dp"
            tools:layout_conversion_absoluteWidth="32dp"
            tools:layout_conversion_absoluteHeight="32dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.68" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_dialog_map"
            android:id="@+id/imageView3"
            tools:layout_conversion_absoluteX="332dp"
            tools:layout_conversion_absoluteY="208dp"
            tools:layout_conversion_absoluteWidth="32dp"
            tools:layout_conversion_absoluteHeight="32dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="@+id/imageView"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            app:layout_constraintVertical_bias="0.13" />

        <TextView
            android:text="General info: info@srijaniitism.org
Hospitality: hospitality@srijaniitism.org
Want to Sponsor?: sponsor@srijaniitism.org"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.83"
            android:fontFamily="cursive"
            android:textColor="@android:color/white"
            android:textAlignment="center"/>

        <TextView
            android:text="+91-88047 18771
        (Secretary, ISS)"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintVertical_bias="0.52"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:fontFamily="cursive"
            android:textColor="@android:color/white"
            android:textAlignment="center" />

        <TextView
            android:text="ISS room, Student Activity Centre
Indian Institute of Technology (ISM)
Dhanbad, 826004"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:fontFamily="cursive"
            android:textColor="@android:color/white"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.26" />


    </android.support.constraint.ConstraintLayout>

</FrameLayout>
